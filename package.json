{
  "name": "workbook",
  "version": "1.0.0",
  "description": "",
  "author": "Marcin Cyboran",
  "scripts": {
    "pretty:server": "prettier --write \"server/**/*.js\"",
    "pretty:client": "prettier --write \"server/**/*.*(ts|tsx)\"",
    "pretty:check": "prettier --check \"**/*.*(js|jsx|ts|tsx)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx|ts|tsx)\""
    }
  },
  "license": "ISC",
  "dependencies": {
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
